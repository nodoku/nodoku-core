{
  "name": "nodoku-core",
  "version": "0.1.5",
  "description": "basic foundation for nodoku static site generator",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "bin": {
    "nodoku-gen-component-resolver": "./dist/esm/bin/generate-component-resolver.js",
    "nodoku-gen-content-schema": "./dist/esm/bin/generate-content-schema.js",
    "nodoku-gen-skin-schema": "./dist/esm/bin/generate-skin-schema.js",
    "nodoku-gen-component-default-theme": "./dist/esm/bin/generate-component-default-theme.js"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "@ts-stack/markdown": "^1.5.0",
    "js-yaml": "^4.1.0",
    "mustache": "^4.2.0",
    "node-html-parser": "^6.1.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@ts-stack/markdown": "^1.5.0",
    "js-yaml": "^4.1.0",
    "mustache": "^4.2.0",
    "node-html-parser": "^6.1.13",
    "@types/js-yaml": "^4.0.9",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.12.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.3",
    "shx": "^0.3.4"
  },
  "scripts": {
    "dist": "shx rm -rf dist && tsc && shx cp -r src/bin/mustache dist/esm/bin/ && shx cp -r schemas dist && shx cp ./package-dist.json ./dist/package.json && shx cp ./README.md ./dist && shx cp -r ./docs ./dist"
  }
}
